#include <iostream>

using namespace std;

//#include <iostream>

using namespace std;

//recursion function for fabonacci series

int fib(int n){
    if(n<2){
        return 1;
    }
    return fib(n-2)+ fib(n-1);
}


int main()
{
    int a;
     cout<<"enter a number "<<endl;
     cin>>a;
     cout<<"The term in fabonacci sequence  " <<a<< "is "<<fib(a)<<endl;

    return 0;
    }
