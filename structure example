#include <iostream>

using namespace std;

struct employee
{
    int eId;
    char favChar;
    float salary;
    
};

int main()
{
    struct employee tanu;
    tanu .eId =1;
    tanu .favChar = 'S';
    tanu .salary= 100000;
    cout<<"the value is "<<tanu.salary<<endl;
    cout<<"The value is" <<tanu.favChar<<endl;
   

    return 0;
}
