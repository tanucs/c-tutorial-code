#include <iostream>

using namespace std;

int main()
{
    int age;
    cout<<"tell me your age";
    cin>>age;
    if(age<18){
        cout<<"you are not in the party";
        
    }
    else if (age==18){
        cout<<"you are kid ";
    }
    else{
        cout<<"you are come in the party";
    }
    return 0;
}
