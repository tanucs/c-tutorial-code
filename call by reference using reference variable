#include <iostream>

using namespace std;

int sum(int a,int b){
    int c=a+b;
    return c;
}  
    void swapReferenceVar(int &a,int &b){   // temp a b
        int temp=a;           //  4  4 5
        a=b;                 //   4  5 5
        b=temp;              //   4  5 4
        }
        
        //call by reference using pointer
        
    
int main(){ 
    int x=4,y=5;
    //cout<<"the sum of 4 and 5 is "<<sum(a,b);
    cout<<"the value of x is"<<x<<"and the value of is "<<y<<endl;
    swapReferenceVar(x,y);
    //swapPointer(&a,&b);
    cout<<"the value of x is"<<x<<"and the value of is "<<y<<endl;

      return 0;
}
