#include <iostream>

using namespace std;

//inline function
//default arugment
float moneyReceived(int currentMoney, float factor=1.04){
   return currentMoney * factor; 
}




int main()
{
    int a,b;
    
    int money = 100000;
    cout<<"If you have"<<money<<"Rs in your bank account ,you will recive"<<moneyReceived<<"Rs after 1 year";
    cout<<"For VIP:If you have"<<money<<"Rs in your bank account,you will recive"<<(money,110000)<<"Rs after 1 year";
    
    return 0;
}
